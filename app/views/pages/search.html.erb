<h3 class="text-info text-center mr-3">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Search Result: </h3>
<div class=" text-success font-italic">
    <div  class="text-center">
      <% @results_username.each do |result| %>
        <p>User: </p> <%= link_to result.username, result %><br>
      <% end %>

      <% @results_organization.each do |result| %>
        <p>Organization: </p><%= link_to result.name, result %><br>
      <% end %>

      <% @results_event.each do |result| %>
        <p>Event name: </p><%= link_to result.name, result %><br>
        <p>Event Description: </p><%= link_to result.description, result%><br>
      <% end %>

      <% @results_event_creator.each do |result| %>
        <p>Event Creator: </p> <%= link_to result.name, result%><br>
      <% end %>

    </div>
  </div>





